version 42
__lua__
function _init()
    x=10
    y=10
end

function _update()
    if btn(0) then x-=1 end
    if btn(1) then x+=1 end
    if btn(2) then y-=1 end
    if btn(3) then y+=1 end
end

function _draw()
    cls()
    spr(1,x,y)
end

